{
    "data_dir": "cremp/train",
    "split_sizes": [0.9, 0.1, 0.0],
    "internal_coordinates_definitions": "angles",
    "use_atom_features": true,
    "atom_feature_fingerprint_radius": 3,
    "atom_feature_fingerprint_size": 32,
    "atom_feature_embed_size": 128,
    "max_conf": 30,
    "timesteps": 20,
    "variance_schedule": "cosine",
    "variance_scale": 1.0,
    "time_encoding": "gaussian_fourier",
    "num_hidden_layers": 12,
    "hidden_size": 256,
    "intermediate_size": 512,
    "num_heads": 12,
    "position_embedding_type": "cyclic_relative_key",
    "dropout_p": 0.1,
    "decoder": "mlp",
    "loss": "smooth_l1",
    "l2_norm": 0.0,
    "l1_norm": 0.0,
    "circle_reg": 0.0,
    "gradient_clip": 1.0,
    "lr": 1e-3,
    "lr_scheduler": "LinearWarmup",
    "max_epochs": 1000,
    "warmup_epochs": 10,
    "early_stop_patience": 0,
    "use_swa": false,
    "batch_size": 8192
}
