{
    "data_dir": "tetrapeptides",
    "split_sizes": [0.8, 0.1, 0.1],
    "internal_coordinates_definitions": "angles",
    "use_atom_features": false,
    "max_conf": 30,
    "timesteps": 50,
    "variance_schedule": "cosine",
    "variance_scale": 1.0,
    "time_encoding": "gaussian_fourier",
    "num_hidden_layers": 3,
    "hidden_size": 24,
    "intermediate_size": 96,
    "num_heads": 3,
    "position_embedding_type": "cyclic_relative_key",
    "dropout_p": 0.1,
    "decoder": "mlp",
    "loss": "smooth_l1",
    "l2_norm": 0.0,
    "l1_norm": 0.0,
    "circle_reg": 0.0,
    "gradient_clip": 1.0,
    "lr": 5e-5,
    "lr_scheduler": "LinearWarmup",
    "max_epochs": 2000,
    "warmup_epochs": 100,
    "early_stop_patience": 0,
    "use_swa": false,
    "batch_size": 64
}
